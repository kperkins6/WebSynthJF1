<p id="notice"><%= notice %></p>

<h1>Listing Synth Preset Users</h1>

<table>
  <thead>
    <tr>
      <th>Preset</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @synth_preset_users.each do |synth_preset_user| %>
      <tr>
        <td><%= synth_preset_user.preset_id %></td>
        <td><%= synth_preset_user.user_id %></td>
        <td><%= link_to 'Show', synth_preset_user %></td>
        <td><%= link_to 'Edit', edit_synth_preset_user_path(synth_preset_user) %></td>
        <td><%= link_to 'Destroy', synth_preset_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Synth preset user', new_synth_preset_user_path %>
